def print_formatted(number):
    # your code goes here
    L=len(bin(n)[2:])
    for i in range(1,number+1):
        print(str(i).rjust(L,' '),oct(i)[2:].rjust(L,' '),
        hex(i)[2:].upper().rjust(L,' '),bin(i)[2:].rjust(L,' '))

if __name__ == '__main__':
    n = int(input())
    print_formatted(n)
